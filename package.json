{
  "name": "dmeditor",
  "version": "0.1.0-alpha.2",
  "description": "dmeditor is a block-styled visual editor. Data is in json format.",
  "keywords": [
    "online editor",
    "digimaker",
    "WYSIWYG",
    "visual editor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digimakergo/dmeditor.git"
  },
  "license": "GPL v2",
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "html-react-parser": "^2.0.0",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dist": "rm -rf dist/* && cp -r ./package.json README.md ./dist && tsc && cp -r ./src/*.css ./dist && cp -r ./src/blocks/*.css ./dist/blocks"
